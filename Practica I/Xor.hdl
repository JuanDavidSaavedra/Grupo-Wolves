/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a, b=notB, out=orEntry1);
    And(a=notA, b=b, out=orEntry2);
    Or(a=orEntry1, b=orEntry2, out=out);
}